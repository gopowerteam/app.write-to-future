{
  "name": "time-letter",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register server/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --ci --verbose --forceExit --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@nestjs/common": "~6.0.5",
    "@nestjs/core": "~6.0.5",
    "@nestjs/graphql": "~6.0.4",
    "@nestjs/passport": "~6.0.0",
    "@nestjs/platform-express": "~6.0.5",
    "@nestjs/swagger": "~3.0.2",
    "@nestjs/typeorm": "~6.0.0",
    "@nuxtjs/axios": "~5.3.6",
    "@nuxtjs/vuetify": "^0.5.5",
    "apollo-server-express": "~2.4.8",
    "class-transformer": "~0.2.0",
    "class-validator": "~0.9.1",
    "cross-env": "~5.2.0",
    "graphql": "~14.2.1",
    "graphql-tools": "~4.0.4",
    "npm": "^6.9.0",
    "nuxt": "2.3.4",
    "nuxt-ts": "~0.3.0",
    "passport": "~0.4.0",
    "passport-http-bearer": "~1.0.1",
    "pg": "~7.9.0",
    "reflect-metadata": "~0.1.12",
    "rimraf": "~2.6.2",
    "rxjs": "~6.2.2",
    "tslint-config-prettier": "~1.18.0",
    "tslint-config-standard": "~8.0.1",
    "type-graphql": "~0.17.1",
    "typeorm": "~0.2.16",
    "typescript": "~3.4.1",
    "vue-property-decorator": "~8.1.0",
    "vuetify": "^1.5.8",
    "vuetify-loader": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "~7.5.2",
    "@nestjs/testing": "~6.0.5",
    "@types/express": "~4.16.0",
    "@types/jest": "~24.0.11",
    "@types/node": "~11.12.2",
    "@types/supertest": "~2.0.5",
    "commitizen": "~3.0.7",
    "commitlint-config-cz": "~0.11.1",
    "cz-conventional-changelog": "~2.1.0",
    "cz-customizable": "~5.5.3",
    "husky": "~1.3.1",
    "jest": "~24.5.0",
    "nodemon": "~1.18.3",
    "prettier": "~1.14.2",
    "stylus": "~0.54.5",
    "stylus-loader": "~3.0.2",
    "supertest": "~4.0.2",
    "ts-jest": "~24.0.1",
    "ts-loader": "~5.3.3",
    "ts-node": "~8.0.3",
    "tsconfig-paths": "~3.5.0",
    "tslint": "~5.14.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src/server",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
