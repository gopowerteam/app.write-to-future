{
  "name": "time-letter",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register server/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --ci --verbose --forceExit --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@nestjs/common": "^6.8.5",
    "@nestjs/core": "^6.8.5",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.8.5",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.2.0",
    "@nuxt/typescript-runtime": "^0.2.2",
    "@nuxtjs/apollo": "^4.0.0-rc17",
    "@nuxtjs/axios": "^5.8.0",
    "ant-design-vue": "~1.3.17",
    "apollo-client": "^2.6.4",
    "apollo-server-express": "^2.9.7",
    "class-transformer": "~0.2.0",
    "class-validator": "^0.10.2",
    "cross-env": "^6.0.3",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.6",
    "npm": "^6.12.1",
    "nuxt": "^2.10.2",
    "passport": "~0.4.0",
    "passport-http-bearer": "~1.0.1",
    "pg": "^7.12.1",
    "reflect-metadata": "~0.1.12",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.2",
    "tslint-config-prettier": "~1.18.0",
    "tslint-config-standard": "~8.0.1",
    "type-graphql": "~0.17.1",
    "typeorm": "^0.2.20",
    "typescript": "~3.5.2",
    "vue-meta": "^2.3.1",
    "vue-property-decorator": "^8.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@mdi/font": "^4.5.95",
    "@nestjs/testing": "^6.8.5",
    "@nuxt/typescript-build": "^0.3.2",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.0",
    "@types/supertest": "~2.0.5",
    "commitizen": "^4.0.3",
    "commitlint-config-cz": "^0.12.1",
    "cz-conventional-changelog": "^3.0.2",
    "cz-customizable": "^6.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "material-design-icons-iconfont": "^5.0.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "stylus": "~0.54.5",
    "stylus-loader": "~3.0.2",
    "supertest": "~4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src/server",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
